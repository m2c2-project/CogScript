     <TrialSet TrialNum="6" Feedback="1"/>
  </Block>
  
  
  <Block>
     <TrialSet TrialNum="4" />
	  <TrialSet TrialNum="2" />
  </Block>
  
  <Block MatchPairsTop="4">
     <TrialSet TrialNum="2" />
  </Block>
  
  </cogtask>
   <nextScreen action="exit" />
   
   
    
   </Screen>


</Pack>


<Pack id="symbolSearchScriptPack" startScreen="gamSymbol">
 
 
 
 <Screen id="gamSymbol">
