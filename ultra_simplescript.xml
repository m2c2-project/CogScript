  </Block>
  <Block MatchPairsTop="4">
     <TrialSet TrialNum="1" />
  </Block>
  <Block MatchPairsTop="4">
     <TrialSet TrialNum="1" />
  </Block>
  
  </cogtask>
   <nextScreen action="exit" />
   
   
    
   </Screen>


</Pack>







