  <Block>
     <TrialSet TrialNum="3" MatchPairsTop="3" Lure="1" />
	 <TrialSet TrialNum="3" MatchPairsTop="3" Lure="0" />
	  
  </Block>
  
  <Block MatchPairsTop="4">
