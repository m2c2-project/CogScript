<Dolphin targetVersion="1.2" studyID="ultra_simplescript">



<Launch> <!-- Launch settings. Options for entire survey app are set here. -->

    <savedata combineGameData="false" gameDataType="0"  />

<!-- gamedata type
        0 = use variable names
        1 = no variable names, export everything in a comma delimeted line
-->		

      <main_menu id="start_menu" />
      <timeout min="30" />
      <beepintro title="Time to do a survey!" text="Press the Start button to begin the Beeped Survey" subtext="" button="Start" />  

      <sensors>
	  
	   <gps on="true" time="true" location="true" /> 
	   <light on="true" />
	   <soundRec on="true" />
 
	  </sensors>
 
      <forceBeepVolume volume="25" />
      <forceBeepVolumeOnStartup volume="50" />
	  
	  <server type="sagebridge" enable="false"/> 
